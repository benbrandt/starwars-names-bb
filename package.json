{
  "name": "starwars-names-bb",
  "version": "0.0.0-semantically-released",
  "description": "Get random Star Wars names",
  "main": "dist/index.js",
  "scripts": {
    "commit": "git-cz",
    "check-coverage": " nyc check-coverage --statements 100 --branches 100 --functions 100 --lines 100",
    "report-coverage": "cat ./coverage/lcov.info | codecov",
    "watch:test": "npm t -- -w",
    "test": "nyc node_modules/mocha/bin/_mocha src/index.test.js --compilers js:babel-register",
    "cover": "nyc --reporter=lcov --reporter=text-lcov npm test",
    "prebuild": "rimraf dist",
    "build": "babel --copy-files --out-dir dist --ignore *.test.js src",
    "semantic-release": "semantic-release pre && npm publish && semantic-release post"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/benbrandt/starwars-names-bb.git"
  },
  "keywords": [
    "random",
    "star",
    "wars"
  ],
  "files": [
    "dist",
    "README.md"
  ],
  "author": "Ben Brandt <benjamin.j.brandt@gmail.com> (https://benjaminbrandt.com/)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/benbrandt/starwars-names-bb/issues"
  },
  "homepage": "https://github.com/benbrandt/starwars-names-bb#readme",
  "dependencies": {
    "unique-random-array": "1.0.0"
  },
  "devDependencies": {
    "babel-cli": "6.8.0",
    "babel-preset-es2015": "6.6.0",
    "babel-preset-stage-2": "6.5.0",
    "babel-register": "6.8.0",
    "chai": "3.5.0",
    "codecov.io": "0.1.6",
    "commitizen": "2.8.1",
    "cz-conventional-changelog": "1.1.6",
    "ghooks": "1.2.1",
    "mocha": "2.4.5",
    "nyc": "6.4.4",
    "rimraf": "2.5.2",
    "semantic-release": "^4.3.5"
  },
  "czConfig": {
    "path": "node_modules/cz-conventional-changelog"
  },
  "config": {
    "ghooks": {
      "pre-commit": "npm run cover && npm run check-coverage"
    }
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-2"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "report-dir": "./coverage"
  }
}
